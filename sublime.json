{
    "always_show_minimap_viewport": true,
    "auto_complete": true,
    "auto_complete_commit_on_tab": true,
    "auto_complete_delay": 50,
    "auto_complete_selector": "source - comment",
    "auto_complete_size_limit": 4194304,
    "auto_complete_with_fields": true,
    "auto_indent": true,
    "auto_match_enabled": true,
    "bold_folder_labels": true,
    "caret_extra_bottom": 12,
    "caret_extra_top": 12,
    "caret_extra_width": 2,
    "caret_style": "phase",
    "close_sidebar_if_opened": false,
    "close_windows_when_empty": false,
    "color_scheme": "Packages/Mooon Light Theme/mooon-light-highcontrast.sublime-color-scheme",
    "convert_tabspaces_on_save": true,
    "copy_with_empty_selection": false,
    "default_encoding": "UTF-8",
    "default_line_ending": "system",
    "detect_indentation": true,
    "drag_text": false,
    "draw_indent_guides": true,
    "draw_minimap_border": true,
    "draw_white_space": "trailing",
    "enable_tab_scrolling": true,
    "ensure_newline_at_eof_on_save": true,
    "expand_tabs_on_save": true,
    "fade_fold_buttons": true,
    "fallback_encoding": "UTF-8",
    "find_selected_text": true,
    "findreplace_small": true,
    "fold_buttons": true,
    "folder_exclude_patterns":
    [
        "vendor",
        "node_modules",
        "dist",
        ".coverage",
        ".git"
    ],
    "font_face": "MonoLisa",
    "font_options":
    [
        "no_round",
        "subpixel_antialias",
        "gray_antialias"
    ],
    "font_size": 20,
    "gutter": true,
    "hide_tab_scrolling_buttons": true,
    "highlight_line": true,
    "highlight_modified_tabs": true,
    "hot_exit": false,
    "ignored_packages":
    [
        "AdvancedNewFile",
        "BracketHighlighter",
        "Profile Switcher",
        "SideBarEnhancements",
        "Vintage",
        "Vue Syntax Highlight",
        "Writer Color Scheme",
    ],
    "indent_guide_options":
    [
        "draw_active"
    ],
    "indent_subsequent_lines": true,
    "line_numbers": true,
    "line_padding_bottom": 8,
    "line_padding_top": 8,
    "margin": 16,
    "match_brackets": true,
    "match_brackets_braces": true,
    "match_brackets_content": false,
    "match_brackets_square": true,
    "match_selection": true,
    "match_tags": true,
    "monokai_pro_file_icons": true,
    "monokai_pro_file_icons_monochrome": false,
    "monokai_pro_highlight_open_folders": true,
    "monokai_pro_label_font_size": 16,
    "monokai_pro_minimal": true,
    "monokai_pro_sidebar_font_size": 16,
    "monokai_pro_sidebar_headings": true,
    "monokai_pro_sidebar_lighter": true,
    "monokai_pro_sidebar_row_padding": 4,
    "monokai_pro_small_scrollbar": true,
    "monokai_pro_style_title_bar": true,
    "open_files_in_new_window": false,
    "overlay_scroll_bars": false,
    "preview_on_click": false,
    "remember_open_files": true,
    "save_on_focus_lost": true,
    "scroll_past_end": true,
    "scroll_speed": 5.0,
    "shift_tab_unindent": false,
    "show_full_path": true,
    "show_panel_on_build": false,
    "sidebar_default": true,
    "soda_classic_tabs": false,
    "soda_folder_icons": true,
    "spell_check": false,
    "tab_completion": true,
    "tab_size": 4,
    "tabs_medium": true,
    "theme": "mooon-light.sublime-theme",
    "translate_tabs_to_spaces": true,
    "trim_automatic_white_space": true,
    "trim_trailing_white_space_on_save": true,
    "ui_native_titlebar": true,
    "ui_separator": true,
    "ui_wide_scrollbars": true,
    "use_tab_stops": true,
    "word_separators": "./\\()\"'-:,.;<>~!@#$%^&*|+=[]{}`~?",
    "word_wrap": false,
    "wrap_width": 72,
}
